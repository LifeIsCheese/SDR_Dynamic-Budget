title: Post Event
new-page: true
on-arrival: {!

Q.last_advisor_action = 0;
// thing
for (const c of Q.classes) {
    for (const party of Q.parties) {
        const key = c + '_' + party;
        if (Q[key] < 0) Q[key] = 0;
    }
}
Q.pro_republic = Math.max(0, Math.min(Q.pro_republic, 99));
Q.socialism = Math.max(0, Math.min(Q.socialism, 99));
Q.nationalism = Math.max(0, Math.min(Q.nationalism, 99));

// --- 2. Calculate normalized class voting (avoid repeated Math.round) ---
for (const c of Q.classes) {
    let class_votes = 0;
    for (const party of Q.parties) class_votes += Q[c + '_' + party];
    if (class_votes === 0) class_votes = 1; // Prevent division by zero
    for (const party of Q.parties) {
        const norm = 100 * Q[c + '_' + party] / class_votes;
        Q[c + '_' + party + '_normalized'] = norm;
        Q[c + '_' + party + '_display'] = Math.round(norm);
    }
    // Decay logic only if class_votes is high
    if (class_votes >= 150) {
        let decay = 1;
        if (class_votes >= 200) decay *= 0.99;
        if (class_votes >= 250) decay *= 0.98;
        if (class_votes >= 300) decay *= 0.96;
        if (Q.year >= 1934) decay *= 0.98;
        for (const party of Q.parties) Q[c + '_' + party] *= decay;
    }
}

// --- 3. Calculate party support (cache old_demographics) ---
let total_support = 0;
const useOld = !!Q.old_demographics;
for (const party of Q.parties) {
    let party_support = 0;
    for (const c of Q.classes) {
        party_support += Q[c] * (useOld ? Q[c + '_' + party] : Q[c + '_' + party + '_normalized']);
    }
    Q[party + '_support'] = party_support;
    total_support += party_support;
}
if (total_support === 0) total_support = 1; // Prevent division by zero
for (const party of Q.parties) {
    const norm = Q[party + '_support'] / total_support;
    Q[party + '_normalized'] = norm;
    const votes = Math.round(norm * 100);
    Q[party + '_votes'] = votes;
    Q[party + '_votes_display'] = votes;
}


// --- 4. Clamp faction stats ---
for (const c of Q.factions) {
    Q[c + '_dissent'] = Math.max(0, Math.min(Q[c + '_dissent'], 99));
    Q[c + '_strength'] = Math.max(0, Q[c + '_strength']);
}

Q.unemployed = Math.max(1, Q.unemployed);
Q.reichswehr_strength = Math.max(0, Q.reichswehr_strength);
Q.rb_militancy = Math.max(0, Q.rb_militancy);

// --- 6. Normalize faction strengths (skip if total_strength is 0) ---
let total_strength = Q.left_strength + Q.center_strength + Q.labor_strength + Q.reformist_strength + Q.neorevisionist_strength;
if (total_strength === 0) total_strength = 1;
Q.left_strength = 100 * Q.left_strength / total_strength;
Q.center_strength = 100 * Q.center_strength / total_strength;
Q.labor_strength = 100 * Q.labor_strength / total_strength;
Q.reformist_strength = 100 * Q.reformist_strength / total_strength;
Q.neorevisionist_strength = 100 * Q.neorevisionist_strength / total_strength;

if ((Q.z_relation >= 50 && Q.ddp_relation >= 40 && Q.nazi_urgency > 0 && Q.unemployed > 12) || (Q.z_relation >= 70 && Q.ddp_relation >= 50 && Q.unemployed > 10) || (Q.z_relation >= 40 && Q.ddp_relation >= 30 && Q.nazi_urgency > 1 && Q.unemployed > 20) || (Q.z_relation >= 40 && Q.ddp_relation >= 30 && Q.unemployed > 30)) {
    Q.weimar_willingness = 1;
} else {
    Q.weimar_willingness = 0;
}

if ((Q.spd_in_government == 1) || (Q.spd_toleration == 1)) {
    Q.dnvp_in_government = 0
}

const total_dissent =
    Q.left_strength * Q.left_dissent +
    Q.center_strength * Q.center_dissent +
    Q.labor_strength * Q.labor_dissent +
    Q.reformist_strength * Q.reformist_dissent +
    Q.neorevisionist_strength * Q.neorevisionist_dissent;
Q.dissent = Math.max(0, Math.min(0.01 * total_dissent / total_strength, 0.95));
Q.dissent_percent = Q.dissent * 100;

// --- 8. Forces calculation (cache total_force) ---
Q.sa_force = Q.sa_strength * Q.sa_militancy;
Q.sh_force = Q.sh_strength * Q.sh_militancy;
Q.rb_force = Q.rb_strength * Q.rb_militancy;
Q.rfb_force = Q.rfb_strength * Q.rfb_militancy;
Q.army_force = Q.reichswehr_strength * Q.reichswehr_militancy;
Q.prussia_force = Q.prussian_police_strength * Q.prussian_police_militancy;
const total_force = Q.sa_force + Q.sh_force + Q.rb_force + Q.rfb_force + Q.army_force + Q.prussia_force || 1;
Q.sa_force = Math.round(Q.sa_force / total_force * 100);
Q.sh_force = Math.round(Q.sh_force / total_force * 100);
Q.rb_force = Math.round(Q.rb_force / total_force * 100);
Q.rfb_force = Math.round(Q.rfb_force / total_force * 100);
Q.army_force = Math.round(Q.army_force / total_force * 100);
Q.prussia_force = Math.round(Q.prussia_force / total_force * 100);


// Loyalty forces
Q.loyal_army = Q.army_force * Q.reichswehr_loyalty;
Q.loyal_prussia = Q.prussia_force * Q.prussian_police_loyalty;

if (Q.sapd_formed == 1 && Q.sapd_consolidated != 1 && Q.centrists_resign == 1) {
    Q.labor_dissent += 5/12;
}
if (Q.sapd_formed == 1 && Q.sapd_consolidated != 1 && Q.unions_independent == 1) {
    Q.center_dissent += 5/12;
}
if (Q.sapd_consolidated == 1 && Q.pro_republic >= 75 && Q.kpd_party_leader != "Conciliators") {
    Q.workers_kpd -= 3/12;
    Q.workers_sapd += 3/12;
}
if (Q.nazi_peak || Q.return_to_normalcy || Q.victory_republic || Q.nsdap_banned) {
    Q.capital_nazi = 1;
} else {
    Q.capital_nazi = 0;
}

// Proletariat calculations
Q.proletariat = Q.workers + (Q.rural*Q.rural_prole) + (Q.new_middle*Q.middle_prole) + (Q.unemployed*Q.lumpenprole);
Q.catholic_proletariat = 15;
Q.urban_proletariat = ((Q.workers/Q.proletariat)*85);
Q.rural_proletariat = (((Q.rural*Q.rural_prole)/Q.proletariat)*85);
Q.middle_proletariat = (((Q.new_middle*Q.middle_prole)/Q.proletariat)*85);
Q.lumpenproletariat = (((Q.unemployed*Q.lumpenprole)/Q.proletariat)*85);
for (var party of Q.parties) {
    let norm = 0;
    norm += Q.urban_proletariat * (Q['workers_' + party + '_normalized']/100);
    norm += Q.rural_proletariat * (Q['rural_' + party + '_normalized']/100);
    norm += Q.middle_proletariat * (Q['new_middle_' + party + '_normalized']/100);
    norm += Q.catholic_proletariat * (Q['catholics_' + party + '_normalized']/100);
    norm += Q.lumpenproletariat * (Q['unemployed_' + party + '_normalized']/100);
    Q['proletariat_' + party + '_normalized'] = Math.round(norm);
}

// 1. update dates
if (Q.month_actions >= 1) {
    Q.time += 1;
    Q.month_actions = 0;
    Q.month += 1;
    if (Q.month >= 13) {
        Q.month = 1;
        Q.year += 1;
        if (Q.historical_mode) {
            Q.resources += 2;
        }
    }
    // decrement timers
    for (timer of Q.timers) {
        if (Q[timer+'_timer'] && Q[timer+'_timer'] > 0) {
            Q[timer+'_timer'] -= 1;
        }
    }
    // append to historical party support records
    var party_support_results = {'date': new Date(Q.year, Q.month - 1)};
    for (var party of Q.parties) {
        party_support_results[party] = Q[party + '_normalized']*100;
    }
    Q.party_support_records.push(party_support_results);
    Q.economic_records.push({'date': new Date(Q.year, Q.month - 1),
                             'inflation': Q.inflation,
                             'unemployment': Q.unemployed});
    // change economic stats...
    // we're basically implicity drawing a Philips curve
    // budget deficits lead to some inflation
    Q.economic_growth -= Q.foreign_investment_old;
    if (Q.budget <= 0 && Q.budget > -2 && Q.inflation < 2.5) {
        Q.inflation += Q.inflation_growth;
    }
    if (Q.budget <= -2 && Q.budget > -5 && Q.inflation < 5) {
        Q.inflation += 0.2;
        if (Q.unemployed >= 12) {
            Q.inflation -= Q.inflation_growth;
        }
    }
    if (Q.budget <= -5 && Q.inflation < 10) {
        Q.inflation += Q.inflation_growth*3;
        if (Q.unemployed >= 15) {
            Q.inflation -= Q.inflation_growth;
        }
    }
    if (((Q.budget <= -5 && Q.inflation >= 10) || (Q.hyperinflation >= 1 && Q.budget < 0)) && Q.hyperinflation_enabled) {
        Q.inflation *= (1.2-(Q.unemployed/100)+((Q.budget*-1)/10)+Q.hyperinflation);
        Q.hyperinflation += 0.1;
        Q.workers_spd -= Q.hyperinflation;
        Q.new_middle_spd -= Q.hyperinflation;
        Q.old_middle_spd -= Q.hyperinflation;
        Q.rural_spd -= Q.hyperinflation;
        Q.unemployed_spd -= Q.hyperinflation;
        Q.coalition_dissent += (Q.hyperinflation*2);
    }
    // deflation is bad
    if (Q.inflation < -0.5 && Q.inflation > -5 && Q.unemployed < 15) {
        Q.unemployed += 0.2;
    }
    if (Q.inflation <= -5 && Q.unemployed < 20) {
        Q.unemployed += 0.3;
    }
        // economic growth
    if (Q.economic_growth < -0.5 && Q.unemployed <= 18) {
        Q.unemployed += 0.1;
        if (Q.inflation >= -2) {
            Q.inflation -= Q.inflation_growth;
        }
    }
    if (Q.economic_growth < -5 && Q.unemployed <= 28) {
        Q.unemployed += 0.1;
        if (Q.inflation >= -5) {
            Q.inflation -= Q.inflation_growth;
        }
    }
    if (Q.economic_growth >= 2 && Q.unemployed >= 17) {
        Q.unemployed -= 0.1;
        if (Q.inflation < 3) {
            Q.inflation += Q.inflation_growth;
        }
    }
    if (Q.economic_growth >= 4 && Q.unemployed >= 12) {
        Q.unemployed -= 0.1;
        if (Q.inflation < 4) {
            Q.inflation += Q.inflation_growth;
        }
    }
    if (Q.economic_growth >= 6 && Q.unemployed >= 7) {
        Q.unemployed -= 0.1;
        if (Q.inflation < 6) {
            Q.inflation += Q.inflation_growth;
        }
    }
    if (Q.economic_growth >= 8 && Q.unemployed >= 3) {
        Q.unemployed -= 0.1;
        if (Q.inflation < 8) {
            Q.inflation += Q.inflation_growth;
        }
    }
    if (Q.economic_growth >= 4 && Q.inflation >= 7.5) {
        Q.economic_growth -= 0.1;
    }
    if (Q.economic_growth >= -2.5 && Q.inflation <= -5) {
        Q.economic_growth -= 0.1;
    }
    if (Q.economic_growth >= 7) {
        Q.economic_growth -= 0.1;
        Q.inflation += Q.inflation_growth;
    }
    if (Q.economic_growth >= 10) {
        Q.economic_growth -= 0.2;
        Q.inflation += Q.inflation_growth;
    }
    if (Q.economic_growth >= 12) {
        Q.economic_growth -= 0.3;
        Q.inflation += Q.inflation_growth;
    }
    if (Q.inflation > 5 && Q.economic_growth >= 1) {
        Q.economic_growth -= 0.1;
        if (Q.inflation > 7 && Q.economic_growth >= 2) {
            Q.economic_growth -= 0.2;
            Q.inflation -= Q.inflation_growth;
        }
    }
    if (Q.inflation >= 8) {
        Q.economic_growth -= 0.1;
        Q.inflation -= Q.inflation_growth;
        if (Q.inflation >= 10) {
            Q.inflation -= Q.inflation_growth;
        }
    }
    if (Q.works_program && !Q.return_to_normalcy) {
        if (Q.economic_growth < 0) {
            Q.economic_growth += Q.works_program/20;
        }
        if (Q.inflation < 2) {
            Q.inflation += Q.works_program/10;
            if (Q.inflation < -5) {
                Q.inflation += Q.works_program/20;
            }
        }
        if (Q.unemployed >= 15) {
            Q.unemployed -= Q.works_program/12;
        }
    }
    if (Q.inflation > 1 && Q.inflation < 3 && Q.year >= 1930) {
        Q.economic_growth += 0.05;
    }
    if (Q.inflation < -4 && Q.economic_growth >= -4) {
        Q.economic_growth -= 0.1;
        if (Q.inflation < -6 && Q.economic_growth >= -6) {
            Q.economic_growth -= 0.1;
            Q.unemployed += 0.1;
        }
    }
    if (Q.unemployed < 5 && Q.economic_growth < 5) {
        Q.economic_growth += 0.1;
    }
    if (Q.unemployed >= 20 && Q.inflation >= -4) {
        Q.inflation -= Q.inflation_growth;
    }
    // economy health rating - vibes based
    Q.economic_health = Q.economic_growth*10;
    Q.economic_health -= Q.unemployed;
    if (Q.inflation < 0) {
        Q.economic_health += Q.inflation;
    }
    if (Q.inflation >= 3) {
        Q.economic_health -= (Q.inflation-3);
    }
// Interest Rates, affecting baseline inflation and economic growth
switch (Q.interest_rates) {
    case 2:
        Q.inflation_growth = 0.25;
        if (Q.inflation > Q.economic_growth) Q.inflation -= 0.2;
        if (Q.economic_growth >= 1) Q.economic_growth -= 0.1;
        Q.s_interest = "Very High";
        break;
    case 1:
        Q.inflation_growth = 0.07;
        Q.s_interest = "High";
        break;
    case 0:
        Q.inflation_growth = 0.1;
        Q.economic_growth += 0.1;
        Q.s_interest = "Normal";
        break;
    case -1:
        Q.inflation_growth = 0.15;
        Q.economic_growth += 0.2;
        Q.s_interest = "Low";
        break;
    case -2:
        Q.inflation_growth = 0.2;
        Q.economic_growth += 0.3;
        Q.s_interest = "Very Low";
        break;
}

// Plot changes
const plotMap = {
    "hjalmar": "hjalmar_plot",
    "bruning": "bruning_plot",
    "rohm": "rohm_plot",
    "leow": "leow_plot"
};
if (plotMap[Q.plot_target]) {
    Q[plotMap[Q.plot_target]] += Q.plot_strength;
}

// Tax reform
Q.tax_reform_progress = 1 - (Q.science_bonus / 10);

// budget
Q.budget_eligible = 1;

// Loyalty decay
Q.loyalty_decay_rate = Q.loyalty_decay - (Q.spy_network / 300);
if (Q.loyalty_decay_rate >= 0) {
    Q.prussian_police_loyalty -= Q.loyalty_decay_rate;
    Q.interior_police_loyalty -= Q.loyalty_decay_rate;
}
const loyaltyStrings = ["None", "Bothering", "Alarming", "Urgent"];
if (Q.loyalty_decay_rate === 0) {
    Q.s_loyalty_decay = loyaltyStrings[0];
} else if (Q.loyalty_decay_rate === 0.01) {
    Q.s_loyalty_decay = loyaltyStrings[1];
} else if (Q.loyalty_decay_rate === 0.02) {
    Q.s_loyalty_decay = loyaltyStrings[2];
} else if (Q.loyalty_decay_rate === 0.03) {
    Q.s_loyalty_decay = loyaltyStrings[3];
} else if (Q.loyalty_decay_rate >= 0.04) {
    Q.s_loyalty_decay = "Do something";
}

// Spy strength
const spyStrings = {
    0: "What's that, whats a spy network",
    3: "Fledgling",
    6: "Influential",
    9: "Dominant"
};
if (Q.spy_network in spyStrings) {
    Q.s_spy_network = spyStrings[Q.spy_network];
    if (Q.spy_network === 9) {
        Q.interior_police_loyalty += 0.02;
        Q.prussian_police_loyalty += 0.02;
    }
}

// Hans Luther opinion
const hansStrings = [
    "Hateful",    // 0
    "Spiteful",   // 1
    "Suspicious", // 2
    "Cooperative",// 3
    "Cordial",    // 4
    "Ecstatic"    // 5
];
if (Q.hans_opinion >= 6) {
    Q.hans_string = "Glazing";
    Q.hans_opinion -= 1;
} else if (Q.hans_opinion >= 0 && Q.hans_opinion <= 5) {
    Q.hans_string = hansStrings[Q.hans_opinion];
}

// Assassination partners string
const assassinationPartners = {
    "Police": "Interior police",
    "Prussian": "Interior + Prussian police",
    "Reichsbanner": "Interior + Prussian police + Reichsbanner"
};
if (Q.assassination_partners in assassinationPartners) {
    Q.assassination_partners_string = assassinationPartners[Q.assassination_partners];
}

// Word is bond
Q.bond_debt = 0;
Q.bond_need_to_pay = 0;
Q.list_debt_amount.forEach((amount, i) => {
    if (Q.list_debt_date[i] > 0) {
        Q.bond_debt += amount * 0.3;
        Q.bond_need_to_pay += amount;
    }
});

// Hiding Targets

if (typeof document !== "undefined") {
    const targets = document.getElementById("Targets");
    if (targets) targets.style.display = (Q.plot_target === "None") ? "none" : "block";
}

// Director string
Q.director_s = Q.director_a[Q.director_pointer];

// Economic Goal calculation
Q.economic_goal = (Q.economic_base * Q.tax_reform) -
    ((Q.lower_tax_rates / 4 / Q.inflation) +
     (Q.upper_tax_rates / 3 / Q.inflation) +
     (Q.tariffs / 6 / Q.inflation)) + Q.inflation / 15;

// Far right ramp up
if (Q.violence_timer > 0) {
    if (Q.rohm_plot !== "successful") {
        Q.sa_strength += Q.violence_timer * 3;
        Q.sh_strength += Q.violence_timer * 2.9;
    } else {
        Q.sa_strength += Q.violence_timer * 2;
        Q.sh_strength += Q.violence_timer * 2.1;
    }
}

// Far left ramp up
if (Q.violence_timer > 0) {
    const leftMultiplier = (100 - Q.nationalism) / (Q.leow_plot !== "successful" ? 7 : 9);
    Q.rfb_strength += Q.violence_timer * leftMultiplier;
}

// Military weariness change
if (Q.violence_timer > 0) {
    if (Q.far_right_strength < 50 && Q.weimar_strength < 50 && Q.kpd_strength < 50) {
        Q.military_weariness += 5;
    } else if (Q.military_weariness > 5) {
        Q.military_weariness -= 5;
    }
}

// Military ramp up
if (Q.military_weariness >= 50) {
    Q.reichswehr_strength += (24 - Q.violence_timer) * (15 + (Q.military_strength / 10));
}

// Military availability 
Q.military_available = 1 - (Q.military_weariness / 100);

// Coalition strengths
Q.weimar_strength = Math.round(Q.rb_force + Q.loyal_prussia + (Q.loyal_army * Q.military_available));
Q.kpd_strength = Math.round(Q.rfb_force);
Q.far_right_strength = Math.round(
    Q.sa_force +
    Q.sh_force +
    (Q.prussia_force * (1 - Q.prussian_police_loyalty)) +
    ((Q.army_force * Q.military_available) * (1 - Q.reichswehr_loyalty))
);
Q.military_strength = Math.round(Q.army_force * (1 - Q.military_available));

/// violence

if (Q.violence_timer > 0) {
    Q.violence_events_pointer = 24 - Q.violence_timer;
    Q.violence_events_id = Q.violence_events[Q.violence_events_pointer];
}

    /// economy effects support
    if (Q.unemployed >= 15 && !Q.spd_in_government) {
        Q.workers_spd += (Q.unemployed/8)/12;
        Q.workers_kpd += (Q.unemployed/10)/12;
        Q.unemployed_spd += (Q.unemployed/12)/12;
        Q.unemployed_kpd += (Q.unemployed/8)/12;
        Q.unemployed_nsdap += (Q.unemployed/12)/12;
        if (Q.economic_plan) {
            Q.workers_kpd -= (Q.unemployed/12)/12;
            Q.unemployed_kpd -= (Q.unemployed/12)/12;
            Q.workers_spd += (Q.unemployed/12)/12;
            Q.unemployed_spd += (Q.unemployed/12)/12;
        }
        if (Q.spd_toleration) {
            Q.workers_spd -= (Q.unemployed/12)/12;
            Q.unemployed_spd -= (Q.unemployed/12)/12;
            Q.workers_kpd += (Q.unemployed/12)/12;
            Q.unemployed_kpd += (Q.unemployed/12)/12;
        }
    } else if (Q.unemployed >= 15 && Q.spd_in_government) {
        Q.workers_spd -= (Q.unemployed/10)/12;
        Q.workers_kpd += (Q.unemployed/8)/12;
        Q.unemployed_spd -= (Q.unemployed/10)/12;
        Q.unemployed_kpd += (Q.unemployed/10)/12;
        Q.unemployed_nsdap += (Q.unemployed/12)/12;
        if (Q.economic_plan) {
            Q.workers_kpd -= (Q.unemployed/12)/12;
            Q.unemployed_kpd -= (Q.unemployed/12)/12;
            Q.workers_spd += (Q.unemployed/12)/12;
            Q.unemployed_spd += (Q.unemployed/12)/12;
        }
    }
    if (Q.inflation > 5) {
        Q.new_middle_dvp += (Q.inflation/2)/12;
        Q.new_middle_other += (Q.inflation/3)/12;
        Q.old_middle_dvp += (Q.inflation/2)/12;
        Q.old_middle_other += (Q.inflation/3)/12;
        Q.rural_other += (Q.inflation/4)/12;
        Q.rural_dvp += (Q.inflation/5)/12;
        Q.workers_ddp += (Q.inflation/5)/12;
        if (Q.spd_in_government) {
            Q.new_middle_spd -= (Q.inflation/4)/12;
            Q.new_middle_ddp += (Q.inflation/5)/12;
            Q.old_middle_spd -= (Q.inflation/4)/12;
            Q.old_middle_ddp += (Q.inflation/6)/12;
            Q.rural_spd -= (Q.inflation/5)/12;
            Q.workers_spd -= (Q.inflation/5)/12;
        }
    }
    if (Q.economic_growth > 5 && Q.spd_in_government) {
        Q.workers_spd += (Q.economic_growth/4)/12;
        Q.new_middle_spd += (Q.economic_growth/2)/12;
        Q.old_middle_spd += (Q.economic_growth/3)/12;
        if (!Q.volkspartei) {
            Q.old_middle_spd -= (Q.economic_growth/6)/12;
        }
        Q.rural_spd += (Q.economic_growth/5)/12;
        Q.unemployed_spd += (Q.economic_growth/5)/12;
    } else if (Q.economic_growth > 5 && !Q.spd_in_government) {
        Q.workers_ddp += (Q.economic_growth/5)/12;
        Q.new_middle_ddp += (Q.economic_growth/3)/12;
        Q.new_middle_dvp += (Q.economic_growth/4)/12;
        Q.old_middle_dvp += (Q.economic_growth/2)/12;
        Q.rural_dvp += (Q.economic_growth/5)/12;
    }
    // factions!
    if (Q.left_dissent < 50 && Q.left_strength < 50) {
        Q.left_dissent += 4/12;
        if (Q.reformist_strength >= 50) {
            Q.left_dissent += 4/12;
        }
        if (Q.centrists_resign) {
            Q.left_dissent += 4/12;
