    if (Q.assassination_partners == "Prussian") {
        Q.assassination_partners_string = "Interior + Prussian police";
    }
    if (Q.assassination_partners == "Reichsbanner") {
        Q.assassination_partners_string = "Interior + Prussian police + Reichsbanner";
    }
    /// word is bond
    Q.counter = 0;
Q.bond_debt = 0;
Q.bond_need_to_pay = 0;
for (i in Q.list_debt_amount) {
  if (Q.list_debt_date[Q.counter] > 0) {
    Q.bond_debt += (Q.list_debt_amount[Q.counter] * 0.1)
    Q.bond_need_to_pay += (Q.list_debt_amount[Q.counter])
  }
  Q.counter +=1
}
/// Hiding Targets
if (Q.plot_target == "None") {
    document.getElementById("Targets").style.display="none";
} else {
    document.getElementById("Targets").style.display="block";
}
    ///string but director
    Q.director_s = Q.director_a[Q.director_pointer];
        /// Economic Goal calculation
    Q.economic_goal = ( (Q.economic_base * Q.tax_reform ) - ( (Q.lower_tax_rates / 4 / Q.inflation) + (Q.upper_tax_rates / 3 / Q.inflation) + (Q.tariffs / 6 / Q.inflation) )) + Q.inflation / 15;
    /// economy effects support
    if (Q.unemployed >= 15 && !Q.spd_in_government) {
        Q.workers_spd += (Q.unemployed/8)/12;
        Q.workers_kpd += (Q.unemployed/10)/12;
        Q.unemployed_spd += (Q.unemployed/12)/12;
